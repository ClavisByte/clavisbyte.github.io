{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "strictNullChecks": true,
    "paths": {
      "@components/*": ["src/components/*"],
      "@constants/*": ["src/content/*"],
      "@utils/*": ["src/utils/*"],
      "@layouts/*": ["src/styles/*"],
      "@icons/*": ["src/icons/*"],
      "@logo/*": ["src/icons/logo/*"],
      "@/*": ["src/*"]
    }
  },
  "include": ["src/**/*"]
}
